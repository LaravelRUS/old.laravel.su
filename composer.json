{
    "name": "laravel-russia/laravel.su",
    "type": "project",
    "description": "Исходный код сайта русскоязычного сообщества Laravel.su",
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com",
            "homepage": "https://tabuna.github.io"
        },
        {
            "name": "Nadezhda Gosteva"
        },
        {
            "name": "Dmitriy Skirta"
        }
    ],
    "license": "CC BY-NC-SA 4.0",
    "require": {
        "php": ">=8.1",
        "cagilo/cagilo": "^3.2",
        "doctrine/dbal": "^3.7",
        "esplora/spire": "0.0.1",
        "guzzlehttp/guzzle": "^7.2",
        "hotwired-laravel/turbo-laravel": "^1.12",
        "intervention/image": "^3.4",
        "jolicode/jolitypo": "^1.4",
        "laravel-notification-channels/telegram": "^5.0",
        "laravel-notification-channels/webpush": "^7.1",
        "laravel/framework": "^10.31",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.5",
        "laravel/socialite": "^5.9",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "orchid/platform": "^14.17",
        "overtrue/laravel-like": "^5.2",
        "spatie/laravel-activitylog": "^4.7",
        "spatie/laravel-backup": "^8.6",
        "spatie/once": "^3.1",
        "symfony/dom-crawler": "^7.0",
        "symfony/yaml": "^7.0",
        "twbs/bootstrap-icons": "^1.11",
        "watson/active": "^7.0"
    },
    "require-dev": {
        "deployer/deployer": "^7.3",
        "fakerphp/faker": "^1.9.1",
        "laravel/envoy": "^2.9",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
